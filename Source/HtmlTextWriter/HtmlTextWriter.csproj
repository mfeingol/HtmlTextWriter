<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard1.4;netstandard2.0;netstandard2.1</TargetFrameworks>
    <AssemblyName>HtmlTextWriter</AssemblyName>
    <RootNamespace>System.Web.UI</RootNamespace>
    <Authors>Max Feingold</Authors>
    <Company>Max Feingold</Company>
    <Version>2.0.0</Version>
  </PropertyGroup>

  <PropertyGroup>
    <Title>HtmlTextWriter</Title>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Copyright>(c) 2019 Max Feingold</Copyright>
    <RepositoryUrl>https://github.com/mfeingol/htmltextwriter</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <Description>.NET Standard-compatible replacement for System.Web.UI.HtmlWriter</Description>
    <PackageReleaseNotes>1.0.0
===
- Initial Release

2.0.0
===
- Don't dispose inner TextWriter</PackageReleaseNotes>
    <PackageProjectUrl>https://github.com/mfeingol/htmltextwriter</PackageProjectUrl>
    <NeutralLanguage>en-US</NeutralLanguage>
    <AssemblyVersion>2.0.0.0</AssemblyVersion>
    <FileVersion>2.0.0.0</FileVersion>
  </PropertyGroup>

</Project>
